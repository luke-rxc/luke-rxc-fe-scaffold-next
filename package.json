{
  "name": "with-typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "env-cmd -f enviornments/.env.development next",
    "build": "env-cmd -f enviornments/.env.production next build",
    "build:dev": "env-cmd -f enviornments/.env.development next build",
    "start": "env-cmd -f enviornments/.env.production next start",
    "export": "env-cmd -f enviornments/.env.production next build && next export",
    "upload": "yarn run export && aws s3 sync out s3://rxc-next-sample",
    "deploy": "serverless",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "lint": "eslint --ext .tsx,.ts,.js --quiet src/",
    "lint:fix": "eslint --fix --ext .tsx,.ts,.js src/",
    "server": "npx json-server ./mock_up/data.json --port 4000"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "next": "latest",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.16.1",
    "react-redux": "^7.2.4",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.21",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/styled-components": "^5.1.10",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "babel-eslint": "^10.1.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.3.0",
    "serverless": "^2.46.0",
    "typescript": "4.0"
  },
  "license": "MIT"
}
